<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="500.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ezdollazbet.view.StartUpController">
   <children>
      <TextField fx:id="loginField" layoutX="101.0" layoutY="127.0" prefHeight="32.0" prefWidth="189.0" />
      <PasswordField fx:id="passwordField" layoutX="101.0" layoutY="238.0" prefHeight="32.0" prefWidth="189.0" />
      <Label layoutX="176.0" layoutY="110.0" text="Login" />
      <Label layoutX="176.0" layoutY="217.0" prefHeight="17.0" prefWidth="38.0" text="Hasło" />
      <Button fx:id="logInButton" layoutX="146.0" layoutY="308.0" mnemonicParsing="false" onAction="#logIn" text="Zaloguj się">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button fx:id="registerButton" layoutX="140.0" layoutY="398.0" mnemonicParsing="false" onAction="#register" text="Zarejestruj się">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Label layoutX="112.0" layoutY="375.0" text="Jeśli nie masz konta stwórz nowe:" />
   </children>
</AnchorPane>
